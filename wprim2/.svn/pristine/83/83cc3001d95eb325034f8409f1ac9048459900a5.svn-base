<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<jsp:include page="/include/journalhome.jsp"></jsp:include>
<div id="right">
    <div class="r_top"></div>
      <div class="r_main">
       <div style="margin-bottom: 5px;">WPRIM &gt;
          <a href="index.jsp">Home</a>&gt;
          <span> Journal Create</span>
        </div>
    	<form action="admin/journal/save">
    		<h1>Basic Information</h1>
    		<label>JournalTitle:</label>
			<input type="text" name="journalTitle" class="form-control"/>*<br>
			<label>VemacularJournalTitle:</label>
			<input type="text" name="VernacularJournalTitle" class="form-control"/><br>
    		<label>Country:</label>
			<select  name="country" class="form-control">
				<option value="China" >China</option>
				<option value="USA" >USA</option>
			</select><br>
			<label>publisher:</label>
			<input type="text" name="publisher" class="form-control"/>*<br>
			<label>ISSN:</label>
			<input type="text" name="ISSN" class="form-control"/>*<br>
			<label>EIISN:</label>
			<input type="text" name="EISSN" class="form-control"/><br>
			<label>ElectronicLinks:</label>
			<input type="text" name="ElectronicLinks" class="form-control"/><br>
			<label>EditorInChief:</label>
			<input type="text" name="EditorInChief" class="form-control"/><br>
			<label>EditorEmail:</label>
			<input type="text" name="EditorEmail" class="form-control"/><br>
			<label>Abbreviation:</label>
			<input type="text" name="Abbreviation" class="form-control"/><br>
			<label>StartYear:</label>
			<input type="text" name="StartYear" class="form-control"/><br>
			<label>SuspendedTime:</label>
			<input type="text" name="suspendedtime" class="form-control"/><br>
			<label>Description:</label>
			<textarea name="Description" class="form-control"></textarea><br>
			<label>Issue:</label>
			<input type="text" name="issue" class="form-control"/><br>
			<label>Start_Issue:</label>
			<input type="text" name="start_issue" class="form-control" value="1"/><br>
			<label>Volume:</label>
			<input type="text" name="volume" class="form-control"/><br>
			<label>Start_Volume:</label>
			<input type="text" name="start_volume" class="form-control" value="1"/><br>
			<label>NewsRoom:</label>
			<input type="text" value="edit" name="edit_department" class="form-control"/><br>
			<label>PubDate:</label>
			<input name="PubDate" class="form-control"><br>
			<label>CRTIME:</label>
			<input name="CRTIME" class="form-control"><br>
			<label>CRUSER:</label>
			<input type="text" name="CRUSER" class="form-control"/><br>
			<label>FirstLetter:</label>
			<input type="text" name="FIRSTLETTER" class="form-control"/><br>
			<label>CountrySimple:</label>
			<input type="text" name="CountrySimple" class="form-control"/><br>
			<label>Review_Status:</label>
			<input type="text" name="review_status" class="form-control"/><br>
			<label>Hc:</label>
			<input type="text" name="hc" class="form-control"/><br>
			<button class="btn btn-info" value="Submit">Submit</button>&nbsp
			<button class="btn btn-info" value="Cancel" onclick="window.close">Cancel</button>
    	</form>
    </div>
   	<div class="r_bot"></div>
  </div>
  </div>
