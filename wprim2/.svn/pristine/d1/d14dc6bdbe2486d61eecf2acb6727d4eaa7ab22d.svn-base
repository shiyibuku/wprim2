<jsp:include page="/include/journalhome.jsp"></jsp:include>
<div id="right">
    <div class="r_top"></div>
      <div class="r_main">
       <div style="margin-bottom: 5px;">WPRIM &gt;
          <a href="index.jsp">Home</a>&gt;
          <span> Journal Create</span>
        </div>
    	<form method="post" action="admin/journal/saveArticle" enctype="multipart/form-data">
    		<label>Article Title(in English):</label>
			<input type="text" name="article.ArticleTitle" class="form-control"/>
			<br>
			<label>Vernacular Title:</label>
			<input type="text" name="article.VernacularTitle" class="form-control"/>
			<br>
			<label>MeSH:</label>
			<input type="text" name="article.MeSH" class="form-control"/>
			<br>
			<label>First Page:</label>
			<input type="text" name="article.FirstPage" class="form-control"/>
			<br>
			<label>Last Page:</label>
			<input type="text" name="article.LastPage" class="form-control"/>
			<br>
			<label>Language:</label>
			<input type="text" name="article.Language" class="form-control"/>
			<br>
			<label>Auther:</label>
			<input type="text" name="article.Auther" class="form-control"/>
			<br>
			<label>CollectiveName:</label>
			<input type="text" name="article.CollectiveName" class="form-control"/>
			<br>
			<label>Affiliation:</label>
			<input type="text" name="article.Affiliation" class="form-control"/>
			<br>
			<label>Publication Type:</label>
			<input type="text" name="article.PublicationType" class="form-control"/>
			<br>
			<label>ArticleId-DOI:</label>
			<input type="text" name="article.DOI" class="form-control"/>
			<br>
			<label>eg:10.1005/s-2007-985171</label>
			<br><br>
			<label>ArticleId-URL:</label>
			<input type="text" name="article.URL" class="form-control"/>
			<br>
			<label>eg:http://www.baidu.com</label>
			<br><br>
			<label>Keywords:</label>
			<input type="text" name="article.Keywords" class="form-control"/>
			<br>
			<label>Abstract:</label>
			<input type="text" name="article.Abstract" class="form-control"/>
			<br>
			<button class="btn btn-info" value="Submit">Submit</button>&nbsp
			<button class="btn btn-info" value="Cancel" onclick="window.close">Cancel</button>
    	</form>
    </div>
   	<div class="r_bot"></div>
  </div>
